int led[]= {13,12,11};
void setup() 

  for( int i =0; i<3; i++)
  {
    pinMode(led[i], OUTPUT);
  }
}

void loop() 
{
   for(int i=0; i<2;i++)
   {
    digitalWrite(led[i], HIGH);
    delay(10000);
   }
  delay(2000);
    for(int i=0; i<2;i++)
   {
    digitalWrite(led[i], LOW);
   }
   
  digitalWrite(11, HIGH);
  delay(5000);
  digitalWrite(11, LOW);
  
}
